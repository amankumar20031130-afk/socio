{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://socio-cxuo.onrender.com/api/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}