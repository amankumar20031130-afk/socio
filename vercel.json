{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "frontend/dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://socio-cxuo.onrender.com/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}